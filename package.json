{
  "name": "technical-ai",
  "version": "1.0.0",
  "description": "Assistant IA pour techniciens de maintenance",
  "scripts": {
    "start": "./start.sh",
    "test": "./test.sh",
    "server": "node server.js",
    "frontend": "cd frontend && HTTPS=true npm start",
    "install:all": "npm ci && cd frontend && npm ci",
    "clean": "rm -rf frontend/build ssl node_modules frontend/node_modules",
    "setup:ssl": "mkdir -p ssl && openssl req -x509 -newkey rsa:2048 -keyout ssl/key.pem -out ssl/cert.pem -days 365 -nodes -subj '/CN=localhost'"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "concurrently": "^8.2.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
